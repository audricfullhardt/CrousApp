{
  "expo": {
    "name": "CROUStillantApp",
    "slug": "crousapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.webp",
    "scheme": "CROUStillantApp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Nous avons besoin de votre localisation pour afficher les restaurants les plus proches de vous.",
        "NSLocationAlwaysUsageDescription": "Nous avons besoin de votre localisation pour afficher les restaurants les plus proches de vous.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Nous avons besoin de votre localisation pour afficher les restaurants les plus proches de vous."
      },
      "config": {
        "googleMapsApiKey": "YOUR_IOS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.webp",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_API_KEY"
        }
      },
      "package": "com.audric_dev.CroustillantApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.webp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9ba9ecd9-5215-4106-8ce7-03a77eaa765b"
      }
    },
    "owner": "audric_dev"
  }
}
